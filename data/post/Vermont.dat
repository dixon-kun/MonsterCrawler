void(0)" ng-click="model.showUpload = !model.showUpload">upload resume</a>):</label>\r\n            <textarea name="ResumeText" class="form-control" rows="10" style="resize:vertical;" ng-model="model.resumeText" ng-required="model.IsResumeRequired" ng-minlength="200"></textarea>\r\n            <p ng-if="form.$submitted && form.ResumeText.$error.required" class="help-block" ng-class="{ \'validate-error-message\': form.$submitted && form.ResumeText.$error.required }">\r\n                <span ng-if="form.$submitted && form.ResumeText.$error.required" class="label label-danger"><i class="fa fa-warning"></i> Required</span>\r\n                You must enter the text of your resume to apply to this position.\r\n            </p>\r\n            <p ng-if="form.$submitted && form.ResumeText.$error.minlength" class="help-block validate-error-messages">\r\n                <span class="label label-danger"><i class="fa fa-warning"></i> Resume Below Minimum Length</span>\r\n                Your resume must be a minimum of 200 charaters in length.\r\n            </p>\r\n        </div>\r\n    </div>\n  </script>\n  <script type="text/javascript">\n   var bydConstants = window.bydConstants = {"JobSearchInput":{"BeyondDirectApply":true,"BoostRAJobs":true,"ExcludeJobId":"419438512","FeaturedJobDirectApply":true,"IsGeoPartner":false,"Keywords":"Client Manager","KeywordType":2,"Location":{"City":"Burlington","State":"VT","Country":"US","Latitude":44.49343,"Longitude":-73.22175,"IsEmpty":false},"LocationString":"Burlington, VT","MobileView":false,"PageSize":20,"SearchAlgorithmId":4},"FeaturedJobModel":{"job":{"ActivityUniversalId":"1-419438512","Age":"Today","CanEmail":true,"CanEmailMe":true,"CanSave":true,"CanShare":true,"Company":"Cox Automotive","CompanyLogoImageUrl":"//d95hpgjsuryud.cloudfront.net/_emp/004/004484/004484926/logo/1450885070_130x90.jpg","HasLiked":false,"ImportSourceTypeId":1,"IsFeaturedJob":true,"IsApplyUrlExternal":false,"JobId":"419438512","Likes":0,"Location":"Burli